{
  "coc.preferences.jumpCommand": "CocSplitIfNotOpen",
  "coc.preferences.formatOnType": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "json",
    "python",
    "java",
    "yaml",
    "proto",
    "go"
  ],
  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": [
        "serve"
      ],
      "filetypes": [
        "terraform",
        "tf"
      ],
      "initializationOptions": {},
      "settings": {}
    }
  },
  "suggest.labelMaxLength": 50,
  "suggest.noselect": false,
  "suggest.removeDuplicateItems": true,
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "constructor": "\uf0ad",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "pyright.enable": true,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.diagnosticSeverityOverrides": {
    "reportCallInDefaultInitializer": "none",
    "reportDuplicateImport": "warning",
    "reportGeneralTypeIssues": "none",
    "reportMissingParameterType": "none",
    "reportMissingImports": "error",
    "reportMissingTypeStubs": "none",
    "reportOptionalMemberAccess": "none",
    "reportOptionalOperand": "none",
    "reportOptionalIterable": "none",
    "reportPropertyTypeMismatch": "none",
    "reportTypeCommentUsage": "none",
    "reportUninitializedInstanceVariable": "none",
    "reportUnusedImport": "warning",
    "reportUnusedVariable": "warning",
    "reportUnnecessaryCast": "none",
    "reportUnnecessaryComparison": "none",
    "reportUnnecessaryIsInstance": "none",
    "reportUnnecessaryTypeIgnoreComment": "none"
  },
  "python.pythonPath": "~/.pyenv/shims/python",
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "black",
  "python.linting.enabled": true,
  "python.linting.ruffEnabled": true,
  "python.linting.ruffPath": "ruff",
  "python.sortImports.path": "isort",
  "pyright.organizeimports.provider": "isort",
  "pyright.completion.snippetSupport": true,
  "yaml.format.enable": true,
  "yaml.format.bracketSpacing": false,
  "yaml.validate": false,
  "yaml.schemas": {
    "kubernetes": "/*.yaml"
  },
  "go.goplsPath": "/Users/lingfengzhong/go_repos/bin/gopls",
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-1.8",
      "path": "/Library/Java/JavaVirtualMachines/jdk1.8.0_192.jdk/Contents/Home"
    },
    {
      "name": "JavaSE-11",
      "path": "/Library/Java/JavaVirtualMachines/jdk-11.0.9.jdk/Contents/Home"
    }
  ],
  "yank.enableCompletion": false,
  "yank.priority": 1,
  "explorer.file.hiddenRules": {
    "extensions": [
      "o",
      "a",
      "obj",
      "pyc"
    ],
    "filenames": [
      "venv",
      ".git",
      ".DS_Store",
      "__pycache__"
    ],
    "patternMatches": []
  },
  "explorer.contentWidthType": "win-width",
  "explorer.width": 40,
  "explorer.buffer.root.template": "[icon] [title] [hidden & 1]",
  "explorer.file.root.template": "[icon] [title] [root] [hidden & 1]",
  "explorer.file.child.template": "[git | 2] [selection | clip | 1] [indent][icon | 1] [diagnosticError & 1][filename omitCenter 1][modified][readonly] [linkIcon & 1][link growRight 1 omitCenter 5]",
  "explorer.file.reveal.auto": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.focus": false,
  "explorer.keyMappings.global": {
    "l": false,
    "L": false,
    "h": false,
    "H": false,
    "o": [
      "expandable?",
      [
        "expanded?",
        "collapse",
        "expand"
      ],
      "open"
    ],
    "v": "open:vsplit",
    "s": "open:split",
    "ma": "addFile",
    "mA": "addDirectory",
    "mm": "rename",
    "md": "delete"
  }
}
